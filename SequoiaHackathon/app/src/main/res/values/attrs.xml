<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CollapsibleBlock">
        <attr name="block_title" format="string" />
        <attr name="block_title_layout" format="reference" />
        <attr name="collapsed" format="boolean" />
    </declare-styleable>
    <declare-styleable name="FontStyle">
        <attr name="font" format="enum">
            <enum name="regular" value="0" />
            <enum name="bold" value="1" />
            <enum name="semi_bold" value="2" />
            <enum name="extra_bold" value="3" />
            <enum name="heavy" value="4" />
            <enum name="medium" value="5" />
            <enum name="light" value="6" />
            <enum name="extra_light" value="7" />
            <enum name="thin" value="8" />
        </attr>
    </declare-styleable>
    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean"></attr>
        <attr name="border_width" format="dimension"></attr>
        <attr name="border_color" format="color"></attr>
        <attr name="shadow" format="boolean"></attr>
    </declare-styleable>
    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference"></attr>
    </declare-styleable>


</resources>